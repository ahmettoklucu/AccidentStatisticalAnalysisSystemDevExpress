<body>
    <h2>KAYIT OL</h2>
    <hr />
    <div class="form-horizontal col-8 mx-auto border mt-5 rounded p-5 center-block">
        <div class="form-group">
            <div class="col-12">
                <span>İsim</span>
                <input type="text" class="form-control" id="Name" required />
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-12">
                <span>Soy İsim</span>
                <input type="text" class="form-control" id="SureName" required>
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-12">
                <span>Kullanıcı Adı</span>
                <input type="text" class="form-control" id="UserName" required>
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-12">
                <span>Parola</span>
                <input type="password" class="form-control" id="Password" required>
                <span id="password-error-message" style="color: red;"></span>
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-12">
                <span>Parola Tekrar</span>
                <input type="password" class="form-control" id="RepeatPassword" required>
                <span id="password-error-message2" style="color: red;"></span>
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-12">
                <span>Telefon</span>
                <input type="text" class="form-control" id="PhoneNumber" required>
                <span id="phone-error-message" style="color: red;"></span>
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-12">
                <span>E-Posta</span>
                <input type="email" class="form-control" id="EMail" required>
                <span id="email-error-message" style="color: red;"></span>
            </div>
        </div>

        <div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-5">
                    <button value="Oluştur" id="RegisterButton" class="btn btn-success" />
                </div>
            </div>

        </div>

    </div>

